{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "no-var-requires": "off",
    "no-non-null-assertion": "off",
    "ban-ts-comment": "off"
  }
}
